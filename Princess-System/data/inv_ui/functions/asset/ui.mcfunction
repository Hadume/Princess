#> inv_ui:asset/ui
# UIのデータ
# @within function inv_ui:load

# 戻る
    # 名前
        data modify storage ui:data ui.back.name set value "戻る"
    # 説明
        data modify storage ui:data ui.back.lore set value "メニューに戻ります"
# 前へ
    # 名前
        data modify storage ui:data ui.prev.name set value "前のページ"
    # 説明
        data modify storage ui:data ui.prev.lore set value ""
# 次へ
    # 名前
        data modify storage ui:data ui.next.name set value "次のページ"
    # 説明
        data modify storage ui:data ui.next.lore set value ""
