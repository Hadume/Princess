{
	"env": {
		"dataSource": "GitHub",
		"dependencies": [
			"@vanilla-mcdoc",
			"@vanilla-datapack"
		],
		"feature": {
			"codeActions": true,
			"colors": true,
			"completions": true,
			"documentHighlighting": true,
			"documentLinks": true,
			"foldingRanges": true,
			"formatting": true,
			"hover": true,
			"inlayHint": {
				"enabledNodes": [
					"mcfunction:command_child/unknown"
				]
			},
			"semanticColoring": false,
			"selectionRanges": true,
			"signatures": true
		},
		"gameVersion": "Auto",
		"language": "Default",
		"permissionLevel": 2,
		"plugins": [],
		"mcmetaSummaryOverrides": {},
		"useFilePolling": false
	},
	"format": {
		"blockStateBracketSpacing": {
			"inside": 0
		},
		"blockStateCommaSpacing": {
			"before": 0,
			"after": 0
		},
		"blockStateEqualSpacing": {
			"before": 0,
			"after": 0
		},
		"blockStateTrailingComma": false,
		"eol": "LF",
		"nbtArrayBracketSpacing": {
			"inside": 0
		},
		"nbtArrayCommaSpacing": {
			"before": 0,
			"after": 0
		},
		"nbtArraySemicolonSpacing": {
			"after": 1
		},
		"nbtArrayTrailingComma": false,
		"nbtByteSuffix": "b",
		"nbtCompoundBracketSpacing": {
			"inside": 0
		},
		"nbtCompoundColonSpacing": {
			"before": 0,
			"after": 0
		},
		"nbtCompoundCommaSpacing": {
			"before": 0,
			"after": 0
		},
		"nbtCompoundTrailingComma": false,
		"nbtDoubleOmitSuffix": false,
		"nbtDoubleSuffix": "d",
		"nbtFloatSuffix": "f",
		"nbtListBracketSpacing": {
			"inside": 0
		},
		"nbtListCommaSpacing": {
			"before": 0,
			"after": 0
		},
		"nbtListTrailingComma": false,
		"nbtLongSuffix": "l",
		"nbtShortSuffix": "s",
		"selectorBracketSpacing": {
			"inside": 0
		},
		"selectorCommaSpacing": {
			"before": 0,
			"after": 0
		},
		"selectorEqualSpacing": {
			"before": 0,
			"after": 0
		},
		"selectorTrailingComma": false,
		"timeOmitTickUnit": false
	},
	"lint": {
		"blockStateSortKeys": null,
		"nbtCompoundSortKeys": null,
		"selectorSortKeys": [
			"warning",
			[
				"type",
				"gamemode",
				"gamemodeNeg",
				"team",
				"teamNeg",
				"typeNeg",
				"tag",
				"tagNeg",
				"name",
				"nameNeg",
				"scores",
				"advancements",
				"nbt",
				"nbtNeg",
				"predicate",
				"predicateNeg",
				"level",
				"x_rotation",
				"y_rotation",
				"distance",
				"x",
				"y",
				"z",
				"dx",
				"dy",
				"dz",
				"sort",
				"limit"
			]
		],
		"commandStringQuote": null,
		"nbtKeyQuote": null,
		"nbtPathQuote": [
			"warning",
			false
		],
		"nbtStringQuote": [
			"warning",
			true
		],
		"selectorKeyQuote": [
			"warning",
			false
		],
		"idOmitDefaultNamespace": true,
		"nameOfNbtKey": "^([A-Z\\d]+[a-z\\d]*((.[A-Z\\d]+)*([a-z\\d]+)*)*)+$",
		"nameOfObjective": "^([A-Z\\d]+[a-z\\d]*((.[A-Z\\d]+)*([a-z\\d]+)*)*)+$",
		"nameOfScoreHolder": "^(#*-*[A-Z\\d]+[a-z\\d]*((.[A-Z\\d]+)*([a-z\\d]+)*)*)+$",
		"nameOfTag": "^([A-Z\\d]+[a-z\\d]*((.[A-Z\\d]+)*([a-z\\d]+)*)*)+$",
		"nameOfTeam": "^([A-Z\\d]+[a-z\\d]*((.[A-Z\\d]+)*([a-z\\d]+)*)*)+$",
		"nbtArrayLengthCheck": [
			"warning",
			true
		],
		"nbtBoolean": [
			"warning",
			false
		],
		"nbtListLengthCheck": [
			"error",
			true
		],
		"nbtTypeCheck": [
			"warning",
			"strictly"
		],
		"strictAdvancementCheck": [
			"error",
			true
		],
		"strictAttributeCheck": [
			"error",
			"always"
		],
		"strictBlockCheck": [
			"error",
			"always"
		],
		"strictBlockTagCheck": [
			"error",
			true
		],
		"strictBossbarCheck": [
			"error",
			true
		],
		"strictDimensionTypeCheck": [
			"error",
			"always"
		],
		"strictEnchantmentCheck": [
			"information",
			"always"
		],
		"strictEntityTypeCheck": [
			"error",
			"always"
		],
		"strictEntityTypeTagCheck": [
			"error",
			true
		],
		"strictFluidCheck": [
			"error",
			"always"
		],
		"strictFluidTagCheck": [
			"error",
			true
		],
		"strictFunctionCheck": [
			"error",
			true
		],
		"strictFunctionTagCheck": [
			"error",
			true
		],
		"strictItemCheck": [
			"error",
			"always"
		],
		"strictItemTagCheck": [
			"error",
			true
		],
		"strictLootTableCheck": [
			"error",
			true
		],
		"strictMobEffectCheck": [
			"error",
			"always"
		],
		"strictMotiveCheck": [
			"error",
			"always"
		],
		"strictObjectiveCheck": [
			"error",
			true
		],
		"strictParticleTypeCheck": [
			"error",
			"always"
		],
		"strictPotionCheck": [
			"error",
			"always"
		],
		"strictPredicateCheck": [
			"error",
			true
		],
		"strictRecipeCheck": [
			"error",
			true
		],
		"strictScoreHolderCheck": [
			"information",
			true
		],
		"strictSoundEventCheck": [
			"error",
			"only-default-namespace"
		],
		"strictStorageCheck": [
			"error",
			true
		],
		"strictTagCheck": [
			"error",
			true
		],
		"strictTeamCheck": [
			"error",
			true
		],
		"undeclaredSymbol": [
			{
				"if": [
					{
						"category": [
							"block",
							"entity_type",
							"item"
						],
						"namespace": "minecraft"
					},
					{
						"category": [
							"advancement",
							"bossbar",
							"objective",
							"team"
						]
					}
				],
				"then": {
					"report": "warning"
				}
			},
			{
				"then": {
					"declare": "block"
				}
			}
		]
	},
	"snippet": {
		"executeIfScoreSet": "execute if score ${1:score_holder} ${2:objective} = ${1:score_holder} ${2:objective} $0",
		"summonAec": "summon minecraft:area_effect_cloud ~ ~ ~ {Age: -2147483648, Duration: -1, WaitTime: -2147483648, Tags: [\"${1:tag}\"]}",
		"TellrawBlockNBT": "tellraw @a [{\"text\":\"${1:NBT}\"},{\"block\":\"${2:~ ~ ~}\",\"nbt\":\"${1:NBT}\"}]",
		"TellrawEntityNBT": "tellraw @a [{\"text\":\"${1:NBT}\"},{\"entity\":\"${2:@s}\",\"nbt\":\"${1:NBT}\"}]",
		"TellrawStorageNBT": "tellraw @a [{\"text\":\"${1:NBT}\"},{\"storage\":\"${2:Storage}\",\"nbt\":\"${1:NBT}\"}]"
	}
}
